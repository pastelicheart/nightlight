Teleport.Position = UDim2.new(0.071576342, 0, 0.901814818, 0)
Teleport.Size = UDim2.new(0.104220949, 0, 0.0462962948, 0)
Teleport.Visible = false
Teleport.Font = Enum.Font.SourceSans
Teleport.Text = ""
Teleport.TextColor3 = Color3.fromRGB(0, 0, 0)
Teleport.TextScaled = true
Teleport.TextSize = 14.000
Teleport.TextWrapped = true

UICorner_26.Parent = Teleport

UIGradient_19.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(158, 133, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(89, 75, 144))}
UIGradient_19.Rotation = -90
UIGradient_19.Parent = Teleport

