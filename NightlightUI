do -- ButtonScripts.Teleport
	local script = Instance.new('ModuleScript', ButtonScripts)
	script.Name = "Teleport"
	local function module_script()
		local Code = {}
		
		function Code.Init()
		    local Template = script.Parent.Parent.Parent.Storage.TextButton:Clone()
		    Template.TextLabel.Text = script.Name
		    Template.Name = script.Name
		    Template.Parent = script.Parent.Parent.Parent.MainFrame.MainActions
		    Template.Visible = true
		    local OldCFrame = nil
		    local Debounce = false
		    Template.MouseButton1Click:Connect(function()
		        if script.Parent.Parent.Parent.Teleport.Visible == true then
		            script.Parent.Parent.Parent.Teleport.Visible = false
		        else
		            script.Parent.Parent.Parent.Teleport.Visible = true
		        end
		    end)
		    script.Parent.Parent.Parent.Teleport.MouseButton1Click:Connect(function()
		        if OldCFrame == nil or Debounce == false then
		            local Player = game.Players.LocalPlayer.Character
		            OldCFrame = game.Players.LocalPlayer.Character:GetPrimaryPartCFrame()
		            wait()
		            Player.HumanoidRootPart.CFrame = CFrame.new(-872.748, -32.4244, -527.338)
		            Debounce = true
		            script.Parent.Parent.Parent.Teleport.TextLabel.Text = "Teleport Back"
		        else
		            local Player = game.Players.LocalPlayer.Character
		            wait()
		            Player:SetPrimaryPartCFrame(OldCFrame)
		            Debounce = false
		            script.Parent.Parent.Parent.Teleport.TextLabel.Text = "Teleport to Admin Base"
		        end
		    end)
		end
		
		return Code
	end
	fake_module_scripts[script] = module_script
end
do -- Utils.PlayerInfo
	local script = Instance.new('ModuleScript', Utils)
	script.Name = "PlayerInfo"
	local function module_script()
		local Code = {}
		local Player = game:GetService("Players")
		local LocalPlayer = Player.LocalPlayer
		Code.PlayerName = LocalPlayer.DisplayName
		Code.UserId = LocalPlayer.UserId
		function Code.GetName(players)
		    if not players._Name then
		        players._Name = LocalPlayer.Name
		    end
		    return players._Name
		end
		function Code.GetThumbnail(players)
		    if not players._Thumbnail then
		        local playerImageReturned = nil
		        local updateAttempts = 0
		        while true do
		            updateAttempts = updateAttempts + 1
		            local playerImage, errorAccurred = Player:GetUserThumbnailAsync(LocalPlayer.UserId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size352x352)
		            playerImageReturned = playerImage
		            wait(0.5)
		            if errorAccurred then
		                break
		            end
		            if updateAttempts > 5 then
		                break
		            end		
		        end
		        players._Thumbnail = playerImageReturned
		    end
		    return players._Thumbnail
		end
		
		function Code.GetHeadshot(players)
		    if not players._Thumbnail then
		        local playerImageReturned = nil
		        local updateAttempts = 0
		        while true do
		            updateAttempts = updateAttempts + 1
		            local playerImage, errorAccurred = Player:GetUserThumbnailAsync(LocalPlayer.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size352x352)
		            playerImageReturned = playerImage
		            wait(0.5)
		            if errorAccurred then
		                break
		            end
		            if updateAttempts > 5 then
		                break
		            end		
		        end
		        players._Thumbnail = playerImageReturned
		    end
		    return players._Thumbnail
		end
		return Code
	end
	fake_module_scripts[script] = module_script
end
