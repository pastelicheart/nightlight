do -- Utils.DraggableObject
	local script = Instance.new('ModuleScript', Utils)
	script.Name = "DraggableObject"
	local function module_script()
		local UDim2_new = UDim2.new
		
		local UserInputService = game:GetService("UserInputService")
		
		local DraggableObject 		= {}
		DraggableObject.__index 	= DraggableObject
		
		-- Sets up a new draggable object
		function DraggableObject.new(Object)
		    local self 			= {}
		    self.Object			= Object
		    self.DragStarted	= nil
		    self.DragEnded		= nil
		    self.Dragged		= nil
		    self.Dragging		= false
		
		    setmetatable(self, DraggableObject)
		
		    return self
		end
		
		-- Enables dragging
		function DraggableObject:Enable()
		    local object			= self.Object
		    local dragInput			= nil
		    local dragStart			= nil
		    local startPos			= nil
		    local preparingToDrag	= false
		
		    -- Updates the element
		    local function update(input)
		        local delta 		= input.Position - dragStart
		        local newPosition	= UDim2_new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		        object.Position 	= newPosition
		
		        return newPosition
		    end
		
		    self.InputBegan = object.InputBegan:Connect(function(input)
		        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		            preparingToDrag = true
							--[[if self.DragStarted then
								self.DragStarted()
							end
							
							dragging	 	= true
							dragStart 		= input.Position
							startPos 		= Element.Position
							--]]
		
		            local connection 
		            connection = input.Changed:Connect(function()
		                if input.UserInputState == Enum.UserInputState.End and (self.Dragging or preparingToDrag) then
		                    self.Dragging = false
		                    connection:Disconnect()
		
		                    if self.DragEnded and not preparingToDrag then
		                        self.DragEnded()
		                    end
		
		                    preparingToDrag = false
		                end
		            end)
		        end
		    end)
		
		    self.InputChanged = object.InputChanged:Connect(function(input)
		        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		            dragInput = input
		        end
		    end)
		
		    self.InputChanged2 = UserInputService.InputChanged:Connect(function(input)
		        if object.Parent == nil then
		            self:Disable()
		            return
		        end
		
		        if preparingToDrag then
		            preparingToDrag = false
		
		            if self.DragStarted then
		                self.DragStarted()
		            end
		
		            self.Dragging	= true
		            dragStart 		= input.Position
		            startPos 		= object.Position
		        end
		
		        if input == dragInput and self.Dragging then
		            local newPosition = update(input)
		
		            if self.Dragged then
		                self.Dragged(newPosition)
		            end
		        end
		    end)
		end
		
		-- Disables dragging
		function DraggableObject:Disable()
		    self.InputBegan:Disconnect()
		    self.InputChanged:Disconnect()
		    self.InputChanged2:Disconnect()
		
		    if self.Dragging then
		        self.Dragging = false
		
		        if self.DragEnded then
		            self.DragEnded()
		        end
		    end
		end
		
		return DraggableObject
	end
	fake_module_scripts[script] = module_script
end
do -- Utils.OtherPlayerInfo
	local script = Instance.new('ModuleScript', Utils)
	script.Name = "OtherPlayerInfo"
	local function module_script()
		local Code = {}
		local Player = game:GetService("Players")
		function Code.GetName(players,...)
		    if not players._Name then
		        local player = ...
		        players._Name = player.Name
		    end
		    return players._Name
		end
		function Code.GetThumbnail(players,...)
		    if not players._Thumbnail then
		        local playerImageReturned = nil
		        local updateAttempts = 0
		        while true do
		            updateAttempts = updateAttempts + 1
		            local player = ...
		            local playerImage, errorAccurred = Player:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.AvatarThumbnail, Enum.ThumbnailSize.Size352x352)
		            playerImageReturned = playerImage
		            wait(0.5)
		            if errorAccurred then
		                break
		            end
		            if updateAttempts > 5 then
		                break
		            end		
		        end
		        players._Thumbnail = playerImageReturned
		    end
		    return players._Thumbnail
		end
		
		function Code.GetHeadshot(players)
		    if not players._Thumbnail then
		        local playerImageReturned = nil
		        local updateAttempts = 0
		        while true do
		            updateAttempts = updateAttempts + 1
		            local playerImage, errorAccurred = Player:GetUserThumbnailAsync(player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size352x352)
		            playerImageReturned = playerImage
		            wait(0.5)
		            if errorAccurred then
		                break
		            end
		            if updateAttempts > 5 then
		                break
		            end		
		        end
		        players._Thumbnail = playerImageReturned
		    end
		    return players._Thumbnail
		end
		
		return Code
	end
	fake_module_scripts[script] = module_script
end


-- Scripts:

local function XPKWJR_fake_script() -- NightlightUI.LocalScript 
	local script = Instance.new('LocalScript', NightlightUI)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Player = game.Players.LocalPlayer
	local Parent = script.Parent
	local Utils = Parent:WaitForChild("Utils")
	local Service = require(Utils:WaitForChild'Services')
	local PlayerInfo = require(Utils:WaitForChild'PlayerInfo')
	local DraggableObject	= require(Utils:WaitForChild'DraggableObject')
	local MainFrame = Parent:WaitForChild("MainFrame")
	
	local MainFrameDrag = DraggableObject.new(MainFrame)
	
	MainFrameDrag:Enable()
	local open = true
	local e1
	local mouse = game.Players.LocalPlayer:GetMouse()
	local TweenService = game:GetService("TweenService")
	local tweenInfoSineOut = TweenInfo.new(0.5, Enum.EasingStyle.Sine, Enum.EasingDirection.Out)
	
	local function openFrame()
	    for i,v in pairs(script.Parent:GetDescendants()) do
	        if v:IsA("Folder") then
	            break
	        end
	        if v:IsA("ModuleScript") then
	            break
	        end
	        if v:IsA("LocalScript") then
	            break
	        end
	        if v:IsA("Frame") then
	            TweenService:Create(v, tweenInfoSineOut, { BackgroundTransparency = 0 }):Play()
	        end
	        if v:IsA("TextButton") then
	            TweenService:Create(v, tweenInfoSineOut, { BackgroundTransparency = 0 }):Play()
	            TweenService:Create(v, tweenInfoSineOut, { TextTransparency = 0 }):Play() 
	        end
	        if v:IsA("TextLabel") then
	            TweenService:Create(v, tweenInfoSineOut, { TextTransparency = 0 }):Play()
	        end
	        if v:IsA("ImageLabel") then
	            TweenService:Create(v, tweenInfoSineOut, { ImageTransparency = 0 }):Play()
	        end
	    end
	    MainFrame.AnchorPoint = Vector2.new(1, 0.5)
	    TweenService:Create(MainFrame, tweenInfoSineOut, { AnchorPoint = Vector2.new(0.5, 0.5) }):Play()
	end
	
	local function closeFrame()
	    for i,v in pairs(script.Parent:GetDescendants()) do
	        if v:IsA("Folder") then
	            break
	        end
	        if v:IsA("ModuleScript") then
	            break
	        end
	        if v:IsA("LocalScript") then
	            break
	        end
	        if v:IsA("Frame") then
	            TweenService:Create(v, tweenInfoSineOut, { BackgroundTransparency = 1 }):Play()
	        end
	        if v:IsA("TextButton") then
	            TweenService:Create(v, tweenInfoSineOut, { BackgroundTransparency = 1 }):Play()
	            TweenService:Create(v, tweenInfoSineOut, { TextTransparency = 1 }):Play() 
	        end
	        if v:IsA("TextLabel") then
	            TweenService:Create(v, tweenInfoSineOut, { TextTransparency = 1 }):Play()
	        end
	        if v:IsA("ImageLabel") then
	            TweenService:Create(v, tweenInfoSineOut, { ImageTransparency = 1 }):Play()
	        end
	    end
	    MainFrame.AnchorPoint = Vector2.new(0.5, 0.5)
	    TweenService:Create(MainFrame, tweenInfoSineOut, { AnchorPoint = Vector2.new(0, 0.5) }):Play()
	    wait(0.6)
	    MainFrame.Visible = false
	end
	
	e1=mouse.KeyDown:connect(function(key)
	    if key=="z" then
	        if open == false then
	            openFrame()
	            open = true
	            MainFrame.Visible = true
	        else
	            closeFrame()
	            open = false
	            MainFrame.Visible = false
	        end
	    end
	end)
	
	MainFrame.playerImageBackground.Image = PlayerInfo:GetThumbnail()
	
	local whitelist = false
	local Whitelisted = require(Utils.Whitelisted)
	
	for _,PlayerID in pairs(Whitelisted) do
	    if Player.UserId == PlayerID.ID then
	        whitelist = true
	    end
	end
	if whitelist == false then
	    MainFrame.MainActions.Visible = false
	    MainFrame.Error.Visible = true        
	    MainFrame.MainActions:Destroy()
	else
	    
	end
	
	local GamePlaying = Service.MarketplaceService:GetProductInfo(game.PlaceId) 
	
	local message = string.format("GAME: %s",GamePlaying.Name)
	MainFrame.GameName.Text = string.upper(message.."!")
	
	for i,Module in pairs(Utils.ButtonScripts:GetChildren()) do
	    if Module:IsA("ModuleScript") then
	        local CodeM = {}
	        CodeM = require(Module)
	        if CodeM.Init then
	            local success, ErrorMessage = pcall(CodeM.Init, Module)
	            if not success then
	                warn("Error in Frame (" .. Module.Name .. "): " .. ErrorMessage)
	                MainFrame.MainActions.CanvasSize = UDim2.new(0, 0, 0, MainFrame.MainActions.UIListLayout.AbsoluteContentSize.Y)
	            end
	            if success then
	                MainFrame.MainActions.CanvasSize = UDim2.new(0, 0, 0, MainFrame.MainActions.UIListLayout.AbsoluteContentSize.Y)
	            end
	        end
	    end
	end
	
	local supported = false
	
	if game.GameId == 2788229376 or game.PlaceId == 2788229376 or game.PlaceId == 6799298516 then
	    supported = true
	end
	
	if supported == true then
	    MainFrame.Status.Text = "SUPPORTED"
	    MainFrame.MainActions.Visible = true
	else
	    MainFrame.Status.Text = "UNSUPPORTED"
	    MainFrame.MainActions.Visible = false
	    MainFrame.Error.Visible = true        
	    MainFrame.MainActions:Destroy()
	    MainFrame.Error.Text = "Seems though this game is unsupported by this specific nightlight product, Feel free to rejoin the game or glitch the gui to get rid of it!"
	end
	
	MainFrameDrag.DragStarted = function()
	    MainFrame:TweenSize(UDim2.new(0.322, 0,0.33, 0),'Out', 'Quad', 0.35, true, nil)
	end
		--[[
		MainFrameDrag.Dragged = function(ObjPosition)
		    
		end]]
	
	MainFrameDrag.DragEnded = function()
	    local c = Parent.Storage.ImageLabel:Clone()
	    c.Visible = true
	    c.Parent = MainFrame
	    local len, size = 0.35, nil
	    if MainFrame.AbsoluteSize.X >= MainFrame.AbsoluteSize.Y then
	        size = (MainFrame.AbsoluteSize.X * 1.5)
	    else
	        size = (MainFrame.AbsoluteSize.Y * 1.5)
	    end
	    c:TweenSize(UDim2.new(0, size, 0, size))
	    MainFrame:TweenSize(UDim2.new(0.307, 0,0.315, 0),'Out', 'Quad', len, true, nil)
	    for i = 1, 10 do
	        c.ImageTransparency = c.ImageTransparency + 0.05
	        wait(len / 12)
	    end
	    c:Destroy()
	end
	
	game.StarterGui:SetCore("SendNotification", {
	    Title = "Nightlight UI";
	    Text = "To hide the UI Hit 'X' !";
	    Duration = 6;
	})
	
	Instance.new("BoolValue", Parent).Name = "Loaded"
end
coroutine.wrap(XPKWJR_fake_script)()
local function OHQMBHH_fake_script() -- ListeningPlayer.LocalScript 
	local script = Instance.new('LocalScript', ListeningPlayer)
	local req = require
	local require = function(obj)
		local fake = fake_module_scripts[obj]
		if fake then
			return fake()
		end
		return req(obj)
	end

	local Players = game:GetService("Players")
	local ContextActionService = game:GetService("ContextActionService")
	
	local Gui = script.Parent
	
	local PlayerName = script.Parent.SearchBar.TextBox
	local AutoFill = script.Parent.SearchBar.Autofill
	
	local playerListened = {}
	
	local function listenPlayer(listen)
	    PlayerName.Text = ""
	    AutoFill.Text = "USERNAME"
	    AutoFill.Visible = false
	    local alreadyListened = false
	    for i,v in pairs(playerListened) do
	        if listen == v then
	            alreadyListened = true
	        end
	    end
	    if alreadyListened == true then
	        
	    else
	        table.insert(playerListened, listen)
	        local MainFrame = script.Parent.MainFrame:Clone()
	        MainFrame.Parent = script.Parent
	        MainFrame.Title.Text = listen.Name:upper()
	        local OtherPlayerInfo = require(script.Parent.Parent.Utils.OtherPlayerInfo)
	        MainFrame.playerImageBackground.Image = OtherPlayerInfo:GetThumbnail(listen)
	        MainFrame.Visible = true
	        MainFrame.Close.MouseButton1Down:Connect(function()
	            for i,v in pairs(playerListened) do
	                if listen == v then
	                    table.remove(playerListened, i)
	                end
	            end
	
	            MainFrame:Destroy()
	        end)
	        listen.Character:FindFirstChild("Humanoid").HealthChanged:Connect(function()
	            MainFrame.Heath.Bar:TweenSize(UDim2.new(listen.Character:FindFirstChild("Humanoid").Health/100,0,1,0),"Out","Quint",0.35)
	            MainFrame.Heath.HeathText.Text = listen.Character:FindFirstChild("Humanoid").Health.."/"..listen.Character:FindFirstChild("Humanoid").MaxHealth
	        end)
	        listen.Character:FindFirstChild("BodyEffects").Armor:GetPropertyChangedSignal("Value"):Connect(function()
	            MainFrame.Heath.Bar:TweenSize(UDim2.new(listen.Character:FindFirstChild("BodyEffects").Armor.Value/100,0,1,0),"Out","Quint",0.35)
	            MainFrame.Shield.HeathText.Text = listen.Character:FindFirstChild("BodyEffects").Armor.Value
	        end)
	        listen.Character:WaitForChild("Humanoid").Died:Connect(function()
	            for i,v in pairs(playerListened) do
	                if listen == v then
	                    table.remove(playerListened, i)
	                end
	            end
	
	            MainFrame:Destroy()
	        end)
	        AutoFill.Visible = false
	    end
	end
	
	local function GetPossiblePlayer(String)
	    if String == "" then return nil end
	    local PlayerList = Players:GetPlayers()
	    for _, Player in pairs(PlayerList) do
	        if Player.Name:sub(1, #String):upper() == String:upper() then
	            if String:upper() == Player.Name:upper() then
	                listenPlayer(Player)
	            end
	            return Player
	        end
	    end
	    return nil
	end
	
	local function PlayerNameChanged(Property)
	    local PossiblePlayer = GetPossiblePlayer(PlayerName.Text)
	    script.Parent.SearchBar.TextBox.Text = string.upper(script.Parent.SearchBar.TextBox.Text)
	    if PossiblePlayer then
	        AutoFill.Visible = true
	        AutoFill.Text = string.upper(PossiblePlayer.Name)
	    else
	        AutoFill.Visible = false
	    end
	end
	
	script.Parent.SearchBar.TextBox:GetPropertyChangedSignal("Text"):Connect(PlayerNameChanged)
	
	
end
coroutine.wrap(OHQMBHH_fake_script)()
